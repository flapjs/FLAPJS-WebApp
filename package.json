{
  "name": "flapjs",
  "version": "0.5.0",
  "description": "Flap.js Web App",
  "main": "index.js",
  "browser": "main.js",
  "scripts": {
    "start": "webpack-dev-server --env=development --config webpack.config.js",
    "build": "webpack --env=production --config webpack.config.js",
    "docs": "npm run docs-storybook && npm run docs-coverage",
    "test": "npm run lint && npm run jest",
    "docs-storybook": "build-storybook -c .storybook -o \"docs/storybook\"",
    "docs-coverage": "npm run jest -- --coverage",
    "test-commit": "lint-staged",
    "storybook": "start-storybook",
    "stories": "npm run storybook",
    "jest": "jest --config \".jest/config.js\" --runInBand",
    "jest-update": "npm run jest -- --updateSnapshot",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-fix": "npm run lint-js -- --fix && npm run lint-css -- --fix",
    "lint-js": "eslint \"src/\" --ext \".js\" --ext \".jsx\"",
    "lint-css": "stylelint \"src/**/*.css\"",
    "boilerplate": "node ./tools/boilerplate/boilerplate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flapjs/FLAPJS-WebApp.git"
  },
  "author": "Flap.js Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flapjs/FLAPJS-WebApp/issues"
  },
  "homepage": "https://github.com/flapjs/FLAPJS-WebApp#readme",
  "dependencies": {
    "@ctrl/tinycolor": "^2.5.4",
    "core-js": "^3.3.5",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-storyshots": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@svgr/webpack": "^4.3.3",
    "autoprefixer": "^9.7.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^5.16.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.12.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.11.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
