{
  "name": "flap.js",
  "version": "1.0.0",
  "description": "Repository for flap.js web app project",
  "main": "./dist/bundle.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.config.js",
    "start-landing": "webpack-dev-server --mode development --config webpack.landing.config.js",
    "build": "webpack --mode production --config webpack.landing.config.js; webpack --mode production --config webpack.config.js;",
    "build-landing": "webpack --mode production --config webpack.landing.config.js",
    "build-app": "webpack --mode production --config webpack.config.js",
    "test": "webpack --config webpack.test.config.js && node ./dist/debug/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flapjs/FLAPJS-WebApp.git"
